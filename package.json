{
  "name": "regis-ai-studio",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host --clearScreen false",
    "prelauncher": "npm install",
    "launcher": "vite --host --open --clearScreen false",
