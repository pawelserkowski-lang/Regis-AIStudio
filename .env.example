# Safe Mode - Enable to block dangerous commands like rm -rf, format, etc.
# Default: false (disabled for power users)
SAFE_MODE=false

# Command execution timeout in seconds
# Default: 300 (5 minutes) - increased from 30 seconds
COMMAND_TIMEOUT=300

# Enable/disable logging to files (logs/server_log.txt, logs/chat.log, etc.)
# Default: true (enabled)
ENABLE_LOGGING=true
